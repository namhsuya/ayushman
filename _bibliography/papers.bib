  author = {Ayushman Kumar Banerjee and Rahul Bhattacharya and Chittabrata Mal},
  title = {{HMG}2D: A tool to identify {miRNAs}/drugs/genes associated with diseases like cancers},
  journal = {Meta Gene}
},
